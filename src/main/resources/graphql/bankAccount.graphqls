input BankAccountCreateForm {
    userId: ID
    title: String
    description: String
    accountType: AccountType
    initialBalance: Float
}
input BankAccountEditForm {
    bankAccountId: ID
    title: String
    description: String
    accountType: AccountType
}
type BankAccount {
    bankAccountId: ID
    title: String
    description: String
    accountType: AccountType
    creationDate: String
}
enum AccountType {
    CHECKING
    SAVINGS
    CASH
    CREDIT_CARD
}
