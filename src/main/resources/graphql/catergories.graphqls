input CategoryCreateForm {
    userId: ID
    name: String
    type: CategoryType
}

input CategoryEditForm {
    categoryId: ID
    name: String
    type: CategoryType
}
input SubCategoryCreateForm {
    categoryId: ID
    name: String
}
input SubCategoryEditForm {
    categoryId: ID
    oldName: String
    newName: String
}
type Category {
    categoryId: ID
    name: String
    subCategories: [String]
    type: CategoryType
    balance: Float
}

enum CategoryType {
    EXPENSE
    INCOME
}
